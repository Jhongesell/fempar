function cell_iterator_get_vef_lid_from_gid (this, vef_lid)
  implicit none
  class(cell_iterator_t), intent(in) :: this
  integer(ip)                        :: vef_lid
  integer(ip)                        :: cell_iterator_get_vef_lid_from_gid
  integer(ip)                        :: ivef
  cell_iterator_get_vef_lid_from_gid = -1
  ! Find position of vef_lid in local element
  do ivef = 1, this%get_num_vefs()
    if ( this%get_vef_gid(ivef) == vef_lid ) then
      cell_iterator_get_vef_lid_from_gid = ivef
      return 
    end if
  end do
end function cell_iterator_get_vef_lid_from_gid

function cell_iterator_get_vef_lid_from_ggid (this, vef_gid)
  implicit none
  class(cell_iterator_t), intent(in) :: this
  integer(igp)                       :: vef_gid
  integer(ip)                        :: cell_iterator_get_vef_lid_from_ggid
  integer(ip)                        :: ivef
  cell_iterator_get_vef_lid_from_ggid = -1
  ! Find position of vef_lid in local element
  do ivef = 1, this%get_num_vefs()
    if ( this%get_vef_ggid(ivef) == vef_gid ) then
      cell_iterator_get_vef_lid_from_ggid = ivef
      return 
    end if
  end do
end function cell_iterator_get_vef_lid_from_ggid


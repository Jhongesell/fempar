     tet_lagrangian_reference_fe_compute_number_interior_nodes = 1
  elseif (order == 1) then
     tet_lagrangian_reference_fe_compute_number_interior_nodes = 0
  elseif (dimension == 1) then
     tet_lagrangian_reference_fe_compute_number_interior_nodes = order-1
  elseif (dimension == 2) then
     tet_lagrangian_reference_fe_compute_number_interior_nodes = int((order-2)*(order-1)/2)
  else
     tet_lagrangian_reference_fe_compute_number_interior_nodes = & 
          & this%compute_number_interior_nodes(dimension-1,1)
     do q=2,order-1
        tet_lagrangian_reference_fe_compute_number_interior_nodes =    & 
             & tet_lagrangian_reference_fe_compute_number_interior_nodes + & 
             & this%compute_number_interior_nodes(dimension-1,q)

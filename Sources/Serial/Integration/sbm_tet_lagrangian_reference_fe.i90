function tet_lagrangian_reference_fe_set_number_quadrature_points( this, order, dimension )
  implicit none
  class(tet_lagrangian_reference_fe_t), intent(in)    :: this
  integer(ip)                         , intent(in)    :: order
  integer(ip)                         , intent(in)    :: dimension
  integer(ip) :: tet_lagrangian_reference_fe_set_number_quadrature_points
  tet_lagrangian_reference_fe_set_number_quadrature_points = & 
       & this%compute_number_nodes_scalar( order, dimension )
  if( dimension .eq. 2 ) then
     if( order .eq. 3 ) then
        tet_lagrangian_reference_fe_set_number_quadrature_points = 13
     end if
  elseif( dimension .eq. 3 ) then
     if( order .eq. 2) then
        tet_lagrangian_reference_fe_set_number_quadrature_points = 11
     end if
  end if
end function tet_lagrangian_reference_fe_set_number_quadrature_points

